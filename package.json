{
  "name": "setup-xcode",
  "version": "1.1.1",
  "private": true,
  "description": "setup xcode action",
  "main": "lib/setup-xcode.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinoru/actions-setup-xcode.git"
  },
  "keywords": [
    "actions",
    "xcode",
    "setup"
  ],
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "semver": "^6.1.1"
  },
  "devDependencies": {
    "@actions/io": "^1.0.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.49",
    "@types/semver": "^6.2.1",
    "husky": "^2.3.0",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format",
      "post-commit": "npm prune --production && git add node_modules/* && git commit -m \"Husky commit correct node modules\""
    }
  }
}
